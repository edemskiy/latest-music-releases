{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2019"],
    "outDir": "./build",
    "module": "commonjs",
    "strict": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  //"files": ["custom.d.ts"],
  "types": ["node"]
}
